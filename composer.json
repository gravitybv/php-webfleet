{
  "name": "gravitybv/php-webfleet",
  "description": "A wrapper for the TomTom Business Webfleet.connect API. PHP 8.0+",
  "homepage": "https://github.com/gravitybv/php-webfleet",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "webfleet",
    "tomtom",
    "telematics",
    "api",
    "wrapper"
  ],
  "authors": [
    {
      "name": "Gravity BV",
      "email": "info@gravity.nl"
    }
  ],
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.6",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Webfleet\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webfleet\\Tests\\": "tests"
    }
  }
}
